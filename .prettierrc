{
	"arrowParens": "always",
	"bracketSpacing": true,
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": true,
	"printWidth": 100,
	"proseWrap": "never",
	"quoteProps": "preserve",
	"requirePragma": false,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "all",
	"useTabs": true,
	"overrides": [
		{
			"files": [
				"**/input/*.ts",
				"**/types.ts"
			],
			"options": {
				"printWidth": 180
			}
		}
	]
}